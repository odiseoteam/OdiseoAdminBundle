{% extends '@OdiseoAdmin/Form/theme.html.twig' %}

{% block form_row %}
    <div class="{% if required %}required {% endif %}form-group{% if (not compound or force_error|default(false)) and not valid %} error{% endif %}">
        {{- form_label(form) -}}
        {{- form_widget(form) -}}
        {{- form_errors(form) -}}
    </div>
{% endblock form_row %}